*background:            #1d1f21
*foreground:            #657b83
! *fading:                15
! *fadeColor:             black
*cursorColor:           #6a694d
*pointerColorBackground:#1d1f21
*pointerColorForeground:#6a694d

!! black dark/light
*color0:                #282928
*color8:                #272827

!! red dark/light
*color1:                #983133
*color9:                #985F31

!! green dark/light
*color2:                #879063
*color10:               #6a694d

!! yellow dark/light
*color3:                #5CE022
*color11:               #63B93D

!! blue dark/light
*color4:                #009890
*color12:               #775050

!! cyan dark/light
*color6:                #57815F
*color14:               #37c49a

!! white dark/light
*color7:                #FDF6E3
*color15:               #FDF6E3

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xcursor.theme: Paper
Xcursor.size:  0

XTerm*background: #1d1f21
XTerm*foreground: #FDF6E3
XTerm*reverseVideo: on
XTerm*faceName: xft:Source Code Pro:size=9
XTerm*selectToClipboard: true

URxvt.font: xft:Source Code Pro:size=9
URxvt.depth: 32
URxvt.background: #1d1f21
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
URxvt*background:                     black
URxvt*foreground:                     grey
URxvt*saveLines:                      5000

URxvt.perl-ext-common: default,matcher
URxvt*urlLauncher: uzbl
URxvt.colorUL: #86a2be

! for 'fake' transparency (without Compton) uncomment the following three lines
